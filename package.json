{
  "name": "ember-rocks",
  "description": "An Em(ber) command line utility to help you build an ambitious web application",
  "version": "0.7.0",
  "homepage": "https://github.com/mattma/ember-rocks",
  "author": {
    "name": "Matt Ma",
    "email": "matt@mattmadesign.com",
    "url": "http://mattmadesign.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattma/ember-rocks.git"
  },
  "bugs": {
    "url": "https://github.com/mattma/ember-rocks/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mattma/ember-rocks/blob/master/LICENSE.md"
    }
  ],
  "main": "src/commands/cli",
  "preferGlobal": "true",
  "bin": {
    "em": "./bin/em"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "ember.js",
    "ember",
    "cli",
    "app",
    "kit",
    "app-kit",
    "ember-app-kit",
    "rocks",
    "gulp"
  ],
  "scripts": {
    "test": "node_modules/.bin/mocha $(find test -name \"*.spec.js\")",
    "test-all": "mocha --timeout 8000 --reporter spec test/**/*.spec.js"
  },
  "dependencies": {
    "commander": "^2.2.0",
    "gulp": "^3.8.10",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.2",
    "gulp-util": "^3.0.2",
    "liftoff": "^2.0.0",
    "pretty-hrtime": "^1.0.0",
    "resolve": "^1.0.0",
    "rimraf": "^2.2.8",
    "semver": "^4.2.0",
    "tildify": "^1.0.0"
  },
  "devDependencies": {
    "gulp-jshint": "^1.9.0",
    "gulp-jscs": "^1.3.1",
    "mocha": "*",
    "chai": "*",
    "mkdirp": "*"
  }
}
